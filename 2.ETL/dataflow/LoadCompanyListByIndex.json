{
	"name": "LoadCompanyListByIndex",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "Connexion_CompanyListByIndex",
						"type": "LinkedServiceReference"
					},
					"name": "ReadCompanyListByIndex"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "FilterByIndex"
				}
			],
			"scriptLines": [
				"parameters{",
				"     IndexList as string[]",
				"}",
				"source(output(",
				"          Company as string,",
				"          Ticker as string,",
				"          StockIndex as string,",
				"          UpdateDate as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     ignoreNoFilesFound: false,",
				"     format: 'excel',",
				"     container: 'alphatradersstock',",
				"     fileName: 'CompanyByStockIndex.xlsx',",
				"     sheetName: 'Feuil1',",
				"     firstRowAsHeader: true) ~> ReadCompanyListByIndex",
				"FilterByIndex select(mapColumn(",
				"          Ticker",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"ReadCompanyListByIndex filter(contains($IndexList, #item==StockIndex)) ~> FilterByIndex"
			]
		}
	}
}